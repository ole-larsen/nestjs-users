    "@types/pg": "^8.6.6",
    "@willsoto/nestjs-prometheus": "^5.1.1",
    "axios": "^1.4.0",
    "bcrypt": "^5.1.0",
    "cache-manager": "^5.2.1",
    "class-sanitizer": "^1.0.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "client-oauth2": "^4.3.3",
